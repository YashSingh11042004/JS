?? JS ??
let btns = document.querySelectorAll("a");
 for(btn of btns){
	btn.onclick = sayHello;
	btn.onmouseenter = function(){
		console.log("you entered in range of button");
	}
	console.dir(btn);
}

/*btn.onclick = function(){
	alert("button on click mode");
}*/

function sayHello() {
	alert("Hello!");
}

